{
  "name": "test",
  "version": "1.0.0",
  "private": true,
  "main": "index.js",
  "license": "MIT",
  "workspaces": [
    "front-office",
    "back-office",
    "backend",
    "content-generator"
  ],
  "scripts": {
    "start:dev": "npm -s run tools && yarn workspace content-generator start && concurrently \"yarn workspace backend start:dev\" \"yarn workspace front-office dev\" \"yarn workspace back-office dev\"",
    "tools": "node ./.lyttle_tools/src/app/core/app.js",
    "tools:version:update": "node ./.lyttle_tools/src/app/version-manager/version.js && chmod ug+x ./.git/hooks/*",
    "tools:breaking:accept": "node ./.lyttle_tools/src/app/breaking/accept.js"
  },
  "dependencies": {
    "concurrently": "^7.4.0"
  }
}
